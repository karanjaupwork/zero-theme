\n            {{ name }}\n          </v-tab>\n        </v-tabs>\n      </div>\n\n      <v-app-bar-nav-icon\n        class=\"hidden-md-and-up\"\n        @click=\"drawer = !drawer\"\n      />\n    </v-app-bar>\n\n    <home-drawer\n      v-model=\"drawer\"\n      :items=\"items\"\n    />\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'HomeAppBar',\n\n    components: {\n      HomeDrawer: () => import('./Drawer'),\n    },\n\n    data: () => ({\n      drawer: null,\n      items: [\n        'Home',\n        'About',\n        'Contact',\n        'Pro',\n      ],\n    }),\n  }\n</script>\n\n<style lang=\"sass\">\n  #home-app-bar\n    .v-tabs-slider\n      max-width: 24px\n      margin: 0 auto\n\n    .v-tab\n      &::before\n        display: none\n</style>\n"]}]}